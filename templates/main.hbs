<div class="wraper">
    <header>
        <div class="container">
            <div class="header">
                <ul>
                    <li><a href="" data-id="1">{{intlGet "messages.head.l1"}}</a></li>
                    <li><a href="" data-id="2">{{intlGet "messages.head.l2"}}</a></li>
                    <li><a href="" data-id="3">{{intlGet "messages.head.l3"}}</a></li>
                    <li><a href="" data-id="4">ICO</a></li>
                    <li><a href="" data-id="5">{{intlGet "messages.head.l4"}}</a></li>
                </ul>
            </div>
        </div>
    </header>
    <div id="main"></div>
    <footer>
        <div class="container">
            <div class="footer">
                <a href="http://coinstorm.top/" target="_blank" class="logo"><img src="../img/logo.png" alt=""></a>
                <a href="https://coinmarketcap.com/" target="_blank" class="val footer_link">{{intlGet "messages.footer.l1"}}</a>
                <a href="https://localbitcoins.net/?ch=duaa" target="_blank" class="buy footer_link">{{intlGet "messages.footer.l2"}}</a>
                <div class="lang">
                    <ul class="langs">
                        <li class="locale" style="background-image: url(img/{{locale}}.png)">{{locale}}
                            <ul class="drop">
                                {{#each Locales}}
                                    <li data-id="{{@key}}" style="background: url(img/{{this.title}}.png) no-repeat left 2px center; -webkit-background-size: 27px 18px;
									background-size: 27px 18px; ">{{this.title}}</li>
                                {{/each}}
                                <!--<li style="background: url(img/united-states.png) no-repeat left 2px center; -webkit-background-size: 27px 18px;-->
									<!--background-size: 27px 18px;">eng</li>-->
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
</div>